{
	"name": "vartrace-vscode",
	"publisher": "ozota1",
	"displayName": "vartrace",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.56.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.vartrace",
		"onView:vartrace.sidebar"
	],
	"main": "./dist/index.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.vartrace",
				"title": "vartrace - analyze this file as a script",
				"category": "debug"
			},
			{
				"command": "setVarNameFromCursor",
				"title": "vartrace - trace this variable",
				"category": "debug"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "vartrace-sidebar",
					"title": "Vartrace",
					"icon": "resources/vt.png"
				}
			]
		},
		"views": {
			"vartrace-sidebar": [
				{
					"type": "webview",
					"id": "vartrace.sidebar",
					"name": "VarTrace"
				}
			]
		},
		"menus": {
			"editor/context": [
				{
					"when": "editorFocus",
					"command": "extension.vartrace",
					"group": "VarTrace@1"
				},
				{
					"when": "varTrace.logLoaded",
					"command": "setVarNameFromCursor",
					"group": "VarTrace@2"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run build",
		"build": "npm-run-all build:*",
		"watch": "npm-run-all --parallel watch:*",
		"build:webpack": "webpack",
		"build:dumper": "cpx \"src/dumper/*.*\" dist/dumper",
		"watch:webpack": "webpack -w --mode development",
		"watch:dumper": "cpx -w \"src/dumper/{*/*,*}\" dist/dumper",
		"test": "npm run build && node ./node_modules/vscode/bin/test",
		"package": "vsce package --yarn",
		"rebuild": "npm run rebuild:electron-rebuild && npm run rebuild:evacuation && npm run rebuild:restore-node-ver && npm run rebuild:restore-evacuation",
		"rebuild:electron-rebuild": "electron-rebuild -v 17.4.0 -f -w better-sqlite3",
		"rebuild:restore-evacuation": "node -e \"require('fs').rename('better-sqlite3-electron', 'node_modules/better-sqlite3-electron', err => {console.log(err)})\"",
		"rebuild:evacuation": "node -e \"require('fs').rename('node_modules/better-sqlite3', 'better-sqlite3-electron', err => {console.log(err)})\"",
		"rebuild:restore-node-ver": "npm install better-sqlite3"
	},
	"devDependencies": {
		"@types/better-sqlite3": "^7.5.0",
		"@types/glob": "^7.1.4",
		"@types/jest": "^27.0.2",
		"@types/node": "^12.20.27",
		"@types/react": "^17.0.26",
		"@types/react-dom": "^17.0.9",
		"@types/react-redux": "^7.1.17",
		"@types/sqlite3": "^3.1.7",
		"@types/vscode": "^1.60.0",
		"@typescript-eslint/eslint-plugin": "^4.32.0",
		"@typescript-eslint/parser": "^4.32.0",
		"cpx": "^1.5.0",
		"cross-env": "^7.0.3",
		"css-loader": "^5.2.7",
		"electron": "^13.5.1",
		"electron-rebuild": "^3.2.7",
		"eslint": "^7.32.0",
		"glob": "^7.2.0",
		"jest": "^27.5.0",
		"license-checker": "^25.0.1",
		"npm-run-all": "^4.1.5",
		"style-loader": "^2.0.0",
		"ts-essentials": "^9.0.0",
		"ts-jest": "^27.1.3",
		"ts-loader": "^9.2.6",
		"typescript": "^4.4.3",
		"vscode-test": "^1.6.1",
		"webpack": "^5.56.0",
		"webpack-cli": "^4.8.0"
	},
	"dependencies": {
		"@babel/core": "^7.15.8",
		"@babel/preset-env": "^7.15.8",
		"@babel/preset-typescript": "^7.15.0",
		"@emotion/react": "^11.6.0",
		"@emotion/styled": "^11.6.0",
		"@fortawesome/fontawesome-svg-core": "^1.2.36",
		"@fortawesome/free-solid-svg-icons": "^5.15.4",
		"@fortawesome/react-fontawesome": "^0.1.16",
		"@mui/icons-material": "^5.2.0",
		"@mui/lab": "^5.0.0-alpha.56",
		"@mui/material": "^5.2.0",
		"@reduxjs/toolkit": "^1.6.1",
		"@types/babel__core": "^7.1.16",
		"@types/react-redux": "^7.1.17",
		"babel-jest": "^27.3.1",
		"better-sqlite3": "^7.5.1",
		"kysely": "^0.16.9",
		"material-ui-popup-state": "^1.9.3",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-id-generator": "^3.0.2",
		"react-redux": "^7.2.5",
		"redux": "^4.1.0",
		"sqlite": "^4.0.23",
		"string-argv": "^0.3.1"
	},
	"jest": {
		"extensionsToTreatAsEsm": [
			".ts"
		],
		"testRegex": "/*/.*\\.test\\.ts$",
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js",
			"jsx",
			"json",
			"node"
		]
	}
}